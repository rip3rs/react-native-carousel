    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-react-native": "^3.5.0",
    "eslint-watch": "^4.0.2",
    "flow-bin": "^0.78.0",
    "metro-react-native-babel-preset": "0.48.1",
    "prettier": "^1.15.2"
